  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "@vitejs/plugin-vue": "^2.3.3",
    "eslint": "^8.19.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitepress": "1.0.0-alpha.4",
    "vue-tsc": "^0.34.7"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
